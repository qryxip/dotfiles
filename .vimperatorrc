ignorekeys clear
set hintchars=abcdefghijklmnopqrstuvwxyz
let g:imeoff='cmd'

inoremap <C-a> <Home>
inoremap <C-e> <End>
inoremap <C-n> <Down>
inoremap <C-p> <Up>
inoremap <C-f> <Right>
inoremap <C-b> <Left>
inoremap <A-f> <C-Right>
inoremap <A-b> <C-Left>
inoremap <A-d> <C-Del>

cnoremap <C-i> <Tab>
cnoremap <C-n> <Tab>
cnoremap <C-p> <S-Tab>

nnoremap t :mytabopen about:blank<CR>:open<Space>
command! mytabopen -nargs=1 -description "Open with a new tab" :js liberator.open("<args>", liberator.NEW_TAB);
nnoremap <C-n> :js gBrowser.mTabContainer.advanceSelectedTab(1, true)<CR>
nnoremap <C-p> :js gBrowser.mTabContainer.advanceSelectedTab(-1, true)<CR>
nnoremap d :js gBrowser.removeTab(gBrowser.mCurrentTab)<CR>
nnoremap u :js undoCloseTab()<CR>
nnoremap r :js gBrowser.reload()<CR>
nnoremap R :js gBrower.reload(true)<CR>

nnoremap i <F7>
nnoremap j <Down>
nnoremap k <Up>
nnoremap e tejje.weblio.jp/content/
nnoremap <C-d> <pagedown>
nnoremap <C-u> <pageup>
nnoremap <C-h> <BS>
nnoremap <C-j> <CR>
nnoremap <C-l> r

nnoremap <Up> gT
nnoremap <Down> gt
